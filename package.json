{
  "name": "pose-tagging-web",
  "productName": "posetag",
  "homepage": "./",
  "description": "A simple tool for labeling pose keypoints",
  "author": {
    "name": "daisy.pang",
    "email": "daisyrpanglive@live.com"
  },
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.10.0",
    "immer": "^8.0.0",
    "pixi-filters": "^3.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "zustand": "^3.2.0"
  },
  "build": {
    "appId": "com.electron.posetag",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "portable"
      ]
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "AppImage"
      ]
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env NODE_ENV=production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 4000",
    "desktop-start": "cross-env BROWSER=none nf start -p 3000",
    "electron": "electron .",
    "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000/ electron .",
    "electron-start": "node ./electron-wait-react",
    "electron-pack-linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "electron-pack-win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@inlet/react-pixi": "^6.0.6",
    "cross-env": "^7.0.3",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "fast-glob": "^3.2.4",
    "foreman": "^3.0.1",
    "is-electron": "^2.2.0",
    "normalize-path": "^3.0.0",
    "pixi-viewport": "^4.18.1",
    "pixi.js": "^5.3.7",
    "simple-zustand-devtools": "^1.0.0"
  }
}
